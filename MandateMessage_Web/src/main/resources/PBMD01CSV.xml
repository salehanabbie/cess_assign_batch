<?xml version="1.0" encoding="UTF-8"?>
<beanio xmlns="http://www.beanio.org/2012/03">
	<stream name="PBMD01CSV" format="csv">
		<record name="header" class="com.bsva.commons.model.MndtRespOutstandingPerBankCsvModel" order="1" occurs="1">
			<field name="crdMemberName" default="CREDITORBANK" length="30" padding=" " justify="left"/>
			<field name="memberNo" default="MEMBERNO" length="30" padding=" " justify="left"/>
			<field name="fileName" default="FILENAME" length="35" padding=" " justify="left"/>
			<field name="transType" default="TRANSACTIONTYPE" length="35" padding=" " justify="left"/>
			<field name="mandateReqTransId" default="MANDATEREQUESTTRANSACTIONID" length="35" padding=" " justify="left"/>
			<field name="nrDaysOutstanding" default="NoOFDAYSOUTSTANDING" length="35" padding=" " justify="left"/>
			<field name="serviceId" default="SUBSERVICEID" length="35" padding=" " justify="left"/>
		</record>

		<record name="header" class="com.bsva.commons.model.MandateResponseOutstandingPerBankModel" order="2" occurs="0+">
			<field name="crdMemberName" length="30" padding=" " justify="left"/>
			<field name="memberNo"  length="30" padding=" " justify="left"/>
			<field name="fileName"  length="35" padding=" " justify="left"/>
			<field name="transType" length="35" padding=" " justify="left"/>
			<field name="mandateReqTransId"  length="35" padding=" " justify="left"/>
			<field name="nrDaysOutstanding"  length="35" padding=" " justify="left"/>
			<field name="serviceId"  length="35" padding=" " justify="left"/>
		</record>
	</stream>


</beanio>